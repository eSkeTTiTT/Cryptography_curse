<Application x:Class="Cryptography_curse.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Cryptography_curse"
             xmlns:vm="clr-namespace:Cryptography_curse.ViewModels.Locator"
             StartupUri="Views/Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>

            <vm:Locator x:Key="vmLocator"/>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Infrastructure/Commands/Commands.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
